package arrays;

import java.util.Arrays;

public class Exercicio {

	public static void main(String[] args) {
		
		double notasAlunoA [] = new double [4];
		notasAlunoA[0]=7.8;
		notasAlunoA[1]=8.0;
		notasAlunoA[2]=6.7;
		notasAlunoA[3]=5;
		
		System.out.println(Arrays.toString(notasAlunoA));

	
	
	}
}
