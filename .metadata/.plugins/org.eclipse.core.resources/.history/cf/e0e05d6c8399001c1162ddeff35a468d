package classe.DesafioModulo;

public class Jantar {

	public static void main(String[] args) {

		Comida c1 = new Comida();
		c1.nomeComida = "churrasco";
		c1.peso = 0.0;
		
		Comida c2 = new Comida();
		c2.nomeComida = "arroz";
		c2.peso = 0.0;

		Pessoa p1 = new Pessoa();
		p1.nomePessoa = "Guilherme";
		p1.pesoPessoa = 80.0;
		
		System.out.println(p1.nomePessoa);
		System.out.println("peso antes de comer" + p1.pesoPessoa);

		p1.comer(1);
        System.out.println("peso após comer: " + p1.pesoPessoa);	
	
	}
}
