package classe.DesafioModulo;

public class Jantar {

	public static void main(String[] args) {
		
	Comida c1 = new Comida();
	c1.nome = "churrasco";
	c1.peso = 3.0;
	
	Pessoa p1 = new Pessoa();
    p1.nome = "Guilherme";
    p1.peso = 80.0;
    
    p1.comer(1);
	System.out.printf("%s comeu:%.2fKg de %s", p1.nome, c1.peso, c1.nome);
		
	}
	}

